<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">


    <g:HTMLPanel>

        <g:HTML>
            <p align="center">
                <strong>
                    <font color="#0070C0" size="4">
                        Спасибо за интерес к нашему Учебному центру и заполненную анкету!
                    </font>
                </strong>
            </p>

            <h3 align="center">
                <strong>
                    <font color="#000000">
                        Личный кабинет
                    </font>
                </strong>
            </h3>
        </g:HTML>

        <b:WellForm>
            <g:HTML ui:field="messageHTML"/>
        </b:WellForm>

        <g:HTMLPanel ui:field="resumePanel"/>

        <b:FormActions>
            <b:Button type="PRIMARY" ui:field="editResumeButton">Редактировать</b:Button>
            <b:Button type="PRIMARY" ui:field="submitResumeButton">Отправить</b:Button>
        </b:FormActions>

    </g:HTMLPanel>

</ui:UiBinder>