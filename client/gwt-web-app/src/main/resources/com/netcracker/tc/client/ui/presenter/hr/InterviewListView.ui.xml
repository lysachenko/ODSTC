<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:table="urn:import:com.netcracker.tc.client.ui.widget.interview"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:HTMLPanel>
        <b:Heading size="2">Собеседования</b:Heading>
        <table:InterviewAsyncTableWidget ui:field="interviewTable"/>
        <b:FormActions>
            <b:Button ui:field="addInterviewButton" type="INFO">Добавить</b:Button>
            <!--<b:Button ui:field="editInterviewButton" type="INFO">Редактировать</b:Button>-->
            <b:Button ui:field="deleteInterviewButton" type="INFO">Удалить</b:Button>
            <b:Button ui:field="startRegistrationButton" type="INFO">Начать регистрацию</b:Button>
            <b:Button ui:field="stopRegistrationButton" type="INFO">Остановить регистрацию</b:Button>
            <b:Button ui:field="getInterviewResultsButton" type="INFO">Загрузить результаты собеседований</b:Button>
            <b:Button ui:field="getDetailInfoButton" type="INFO">Детальная информация</b:Button>
        </b:FormActions>
    </g:HTMLPanel>
</ui:UiBinder>